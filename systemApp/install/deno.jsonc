{
  "tasks": {},
  "compilerOptions": {
    "allowJs": false,
    "lib": [
      "deno.window"
    ],
    "strict": true,
    "strictNullChecks": true
  },
  "lint": {
    "rules": {
      "exclude": [
        "no-namespace"
      ]
    }
  },
  "fmt": {
    "options": {
      "useTabs": false,
      "lineWidth": 120,
      "indentWidth": 2,
      "singleQuote": false,
      "proseWrap": "preserve"
    }
  },
  "importMap": "./import_map.json"
}
