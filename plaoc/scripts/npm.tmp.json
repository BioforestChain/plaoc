[
  {
    "buildFromRootDir": "./packages/gateway",
    "mainExports": ".",
    "buildToRootDir": "./build/gateway",
    "importMap": "./import_map.json",
    "name": "@bfsx/gateway",
    "version": "0.1.0",
    "lib": ["es2020"]
  },
  {
    "buildFromRootDir": "./packages/metadata",
    "mainExports": ".",
    "buildToRootDir": "./build/metadata",
    "importMap": "./import_map.json",
    "name": "@bfsx/metadata",
    "version": "0.1.0",
    "lib": ["es2020"]
  },
  {
    "buildFromRootDir": "./packages/typings",
    "mainExports": ".",
    "buildToRootDir": "./build/typings",
    "importMap": "./import_map.json",
    "name": "@bfsx/typings",
    "version": "0.1.0",
    "lib": ["es2020"]
  },
  {
    "buildFromRootDir": "./packages/plugin",
    "mainExports": ".",
    "buildToRootDir": "./build/plugin",
    "importMap": "./import_map.json",
    "name": "@bfsx/plugin",
    "version": "0.1.0",
    "lib": ["es2020", "dom"],
    "shims": {
      "custom": [
        {
          "package": {
            "name": "@bfsx/gateway",
            "version": "^0.1.0"
          },
          "globalNames": [
            "getCallNativeUi",
            "createMessage",
            "postCallNativeUi",
            "registerServiceWorker"
          ]
        }
      ]
    }
  },
  {
    "buildFromRootDir": "./packages/core",
    "mainExports": ".",
    "buildToRootDir": "./build/core",
    "importMap": "./import_map.json",
    "name": "@bfsx/core",
    "version": "0.1.0",
    "lib": ["es2020"],
    "shims": {
      "custom": [
        {
          "package": {
            "name": "@bfsx/gateway",
            "version": "^0.1.0"
          },
          "globalNames": ["netCallNativeService"]
        },
        {
          "package": {
            "name": "@bfsx/metadata",
            "version": "^0.1.0"
          },
          "globalNames": ["MetaData"]
        }
      ]
    }
  },
  {
    "buildFromRootDir": "./packages/vfs",
    "mainExports": ".",
    "buildToRootDir": "./build/vfs",
    "importMap": "./import_map.json",
    "name": "@bfsx/vfs",
    "version": "0.1.0",
    "lib": ["es2020"],
    "shims": {
      "custom": [
        {
          "package": {
            "name": "@bfsx/core",
            "version": "^0.1.0"
          },
          "globalNames": [
            "network",
            {
              "name": "Network",
              "typeOnly": true
            }
          ]
        }
      ]
    }
  }
]
